<style>
    #user-growth-chart {
        width: 100%;
        height: 350px; /* Altezza del grafico */
    }
    @@media (max-width: 768px) {
        .apexcharts-toolbar {
        display: none !important;
        }

        .apexcharts-legend {

            justify-content: center !important;
        }

    }
    .dt-layout-row{
        margin: 1.5%;
        display: flex!important;
        margin-top:0.7rem!important;
        margin-bottom:0rem!important;
    }

    .dt-layout-row.dt-layout-table  {
        display: inline!important;
    }

    .dt-layout-row .dt-layout-end{
            margin-left: auto;
            margin-right: 0!important;
    }

    .dt-input {
        border: 1px solid #ced4da;
        padding: 0.5rem 0.9rem;
        border-radius: 0.2rem!important;
        transition: all 0.5s ease-in-out;
    }

        .dt-input:focus {
            outline: none !important; 
            border-color: #0d6efd; 
            box-shadow: 0 0 10px rgba(13, 110, 253, 0.25);
            background-color: #ffffff; 
        }


        /* Effetto hover in stile Bootstrap */
        .dt-input:hover {
            border-color: #0d6efd; 
            box-shadow: 0 0 10px rgba(13, 110, 253, 0.25);
            background-color: #f8f9fa; 
        }

    label[for^="dt-length"] { 
        font-size: 1rem;
        margin-left: 0.5rem!important;
       
    }

    label[for^="dt-search-0"] {
       margin-right:1rem!important;
    }

    .dt-paging > nav {
        display:flex!important;
    }

    .dt-paging-button {
        display: block!important;
        align-items: center;
        justify-content: center;
        width: 2.25rem;
        height: 2.25rem;
        border: 1px solid #dee2e6; /* Bordo simile a Bootstrap */
        background-color: #f8f9fa; /* Sfondo chiaro */
        color: #212529; /* Testo scuro */
        border-radius: 5px; /* Angoli arrotondati */
        font-size: 16px;
        transition: all 0.2s ease-in-out;
    }

        .dt-paging-button:hover {
            background-color: #e9ecef;
            border-color: #adb5bd;
        }
        /* Pulsante attivo (pagina corrente) */
        .dt-paging-button.current {
            background-color: #0d6efd; /* Blu Bootstrap */
            color: white;
            border-color: #0d6efd;
            font-weight: bold;
        }
        /* Pulsanti disabilitati */
        .dt-paging-button.disabled {
            opacity: 0.5;
            cursor:not-allowed;
        }

        select.dt-input{
            padding:0.2rem!important;
            margin-top:0.4rem!important;
        }

    .menu-hover:hover {
        background-color: #f5f5f5 !important;
        color: black!important;
        font-weight:600!important;
        cursor: pointer;
    }

    .input-hover:focus {
        border-color: #007bff !important;
        color: black !important;
        font-weight: 600!important;
       
    }

    .border-group-account {
        border:none!important;
    }
    
